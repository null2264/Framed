{
  "schemaVersion": 1,

  "id": "framed",
  "version": "${version}",
  "environment": "*",

  "name": "Framed",
  "description": "Blocks with customizable textures",
  "icon": "assets/framed/icon.png",

  "authors": [ "noobstar", "null2264" ],
  "contributors": [ "Haven King" ],
  "contact": {
    "sources": "https://github.com/null2264/Framed",
    "issues": "https://github.com/null2264/Framed/issues",
    "email": "palembani@gmail.com"
  },

  "license": "LGPL-2.1-only",

  "entrypoints": {
    "main": [
      "io.github.null2264.framed.Framed"
    ],
    "client": [
      "io.github.null2264.framed.client.FramedClient"
    ],
    "config": [
      "io.github.null2264.framed.client.FramedConfig"
    ]
  },
  "mixins": [
    "mixins.framed.json"
  ],

  "depends": {
    "fabricloader": "*",
    "fabric": "${fapiVersion}",
    "minecraft": "1.16.x",
    "libgui": "${libguiVersion}",
    "json-model-extensions": "${jmxVersion}",
    "frex": "${frexVersion}"
  },
  "suggests": {
    "canvas": "*",
    "modmenu": "*"
